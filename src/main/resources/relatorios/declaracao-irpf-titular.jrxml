	<queryString language="SQL">
		<![CDATA[SELECT fatura.vencimento,
	beneficiario.cpf,
	beneficiario.nome,
	item_fatura.valor,
	item_fatura.identificacao,
	beneficiario.acomodacao
FROM item_fatura
	INNER JOIN fatura ON 
	 item_fatura.fatura_id = fatura.id 
	INNER JOIN plano ON 
	 fatura.plano = plano.id 
	INNER JOIN beneficiario ON 
	 plano.beneficiario = beneficiario.id
WHERE beneficiario.id = $P{p_beneficiario_id}
AND EXTRACT(YEAR FROM fatura.vencimento) = $P{p_ano}
AND item_fatura.identificacao = 'UNIMED FAMA'
ORDER BY vencimento]]>
	</queryString>
