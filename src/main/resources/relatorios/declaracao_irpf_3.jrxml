	<parameter name="p_beneficiario_id" class="java.lang.Long">
		<parameterDescription><![CDATA[]]></parameterDescription>
	</parameter>
	<parameter name="p_ano" class="java.lang.Integer"/>
	<queryString language="SQL">
		<![CDATA[SELECT public.fatura.vencimento,
	public.beneficiario.nome,
	public.fatura.valor_plano,
	public.beneficiario.cpf
FROM public.fatura
	INNER JOIN public.plano ON 
	 public.fatura.plano = public.plano.id 
	INNER JOIN public.beneficiario ON 
	 public.plano.beneficiario = public.beneficiario.id 
WHERE 
	 public.beneficiario.id = $P{p_beneficiario_id}
AND
	extract(year from public.fatura.vencimento) = $P{p_ano}	 
ORDER BY
	public.fatura.vencimento]]>
	</queryString>
